<%- include("partials/header"); -%>


<h1>Compose</h1>

<form class="" action="/compose" method="post">
    <div class="mb-3">
        <label for="exampleFormControlInput1" class="form-label">Title</label>
        <input type="text" class="form-control" id="exampleFormControlInput1" name="title" autocomplete="off">
    </div>
    <div class="mb-3">
        <label for="exampleFormControlTextarea1" class="form-label">Post</label>
        <textarea class="form-control" name="titleContent" rows="5" cols="30"></textarea>
    </div>
    <button type="submit" class="btn btn-primary" name="button">Publish</button>
</form>



<%- include("partials/footer"); -%>